  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>SELECT 
    COALESCE(NULLIF(UT."assignTo", ''), 'Un-Assigned') AS "UserName",
    D."depId"       AS "DepartmentId",
    D."depName"     AS "DepartmentName",
    UT."taskDate"   AS "TaskDates",
    UT."assignTo"   AS "assignTo",
    COUNT(*)        AS "TotalTask",
    -- Pending should also include Assigned
    SUM(CASE WHEN TS."taskStatusName" IN ('Pending', 'Assigned') THEN 1 ELSE 0 END) AS "Pending",
    SUM(CASE WHEN TS."taskStatusName" = 'In-Process' THEN 1 ELSE 0 END) AS "InProcess",
    -- Force Assigned = 0 (or remove this column if not needed)
    0 AS "Assigned",
    SUM(CASE WHEN TS."taskStatusName" = 'Completed'  THEN 1 ELSE 0 END) AS "Completed",
    SUM(CASE WHEN TS."taskStatusName" = 'Closed'     THEN 1 ELSE 0 END) AS "Closed",
    SUM(CASE WHEN TS."taskStatusName" = 'Hold'       THEN 1 ELSE 0 END) AS "Hold"
FROM "AD"."UserTask" UT
LEFT JOIN "AD"."TaskStatus" TS 
       ON TS."taskStatusId" = UT."taskStatusId"
LEFT JOIN "AD"."Department" D
       ON D."depId" = UT."depId"
GROUP BY 
    COALESCE(NULLIF(UT."assignTo", ''), 'Un-Assigned'),
    D."depId", 
    D."depName",
    UT."taskDate",
    UT."assignTo"
ORDER BY 
    "UserName", 
    D."depName";</value>
  </data>
